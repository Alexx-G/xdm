    "$id": "https://ns.adobe.com/xdm/context/graph-edge",
    "$schema": "http://json-schema.org/draft-06/schema#",
    "title": "XDM Graph Edge",
    "type": "object",
    "description": "Information regarding the relationship between two nodes or vertices such as data provider, and edge information.",
    "meta:extensible": true,
    "meta:abstract": true,
    "meta:intendedToExtend": [
        "https://ns.adobe.com/xdm/context/graphs"
    ],
    "definitions": {
        "graph-edge": {
            "properties": {
                "xdm:edges": {
                    "title": "Array Of Edges",
                    "description": "Array of edges.",
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "xdm:edgeId": {
                                "title": "Edge Identifier",
                                "description": "The unique identifier of the identity associated with this graph edge.",
                                "type": "string",
                                "meta:titleId": "graph-edge##xdm:edgeId##title##49411",
                                "meta:descriptionId": "graph-edge##xdm:edgeId##description##75781"
                            },
                            "xdm:sourceNodeId": {
                                "title": "Source Node Id",
                                "description": "Source node identifier.",
                                "type": "string",
                                "meta:titleId": "graph-edge##xdm:sourceNodeId##title##64001",
                                "meta:descriptionId": "graph-edge##xdm:sourceNodeId##description##43661"
                            },
                            "xdm:targetNodeId": {
                                "title": "Target Node Id",
                                "description": "Target node identifier.",
                                "type": "string",
                                "meta:titleId": "graph-edge##xdm:targetNodeId##title##98741",
                                "meta:descriptionId": "graph-edge##xdm:targetNodeId##description##21581"
                            },
                            "xdm:relation": {
                                "title": "Relation Description",
                                "description": "Relation description.",
                                "type": "string",
                                "meta:titleId": "graph-edge##xdm:relation##title##76811",
                                "meta:descriptionId": "graph-edge##xdm:relation##description##35351"
                            },
                            "xdm:label": {
                                "title": "Label",
                                "description": "Additional description.",
                                "type": "string",
                                "meta:titleId": "graph-edge##xdm:label##title##16101",
                                "meta:descriptionId": "graph-edge##xdm:label##description##8571"
                            },
                            "xdm:dataProvider": {
                                "type": "array",
                                "items": {
                                    "type": "object",
                                    "additionalProperties": false,
                                    "properties": {
                                        "xdm:dataProviderId": {
                                            "title": "Data Provider Id",
                                            "description": "Identifier of the Data Provider..",
                                            "type": "integer",
                                            "meta:titleId": "graph-edge##xdm:dataProviderId##title##75731",
                                            "meta:descriptionId": "graph-edge##xdm:dataProviderId##description##31721"
                                        },
                                        "xdm:firstSeenTimestamp": {
                                            "title": "Relationship Observed Time and Date",
                                            "description": "Time and date the relation was first observed.",
                                            "type": "string",
                                            "format": "date-time",
                                            "meta:titleId": "graph-edge##xdm:firstSeenTimestamp##title##39461",
                                            "meta:descriptionId": "graph-edge##xdm:firstSeenTimestamp##description##74231"
                                        },
                                        "xdm:lastSeenTimestamp": {
                                            "title": "Relationship Observed most recent Time and Date",
                                            "description": "Time and date the relation was last observed.",
                                            "type": "string",
                                            "format": "date-time",
                                            "meta:titleId": "graph-edge##xdm:lastSeenTimestamp##title##64211",
                                            "meta:descriptionId": "graph-edge##xdm:lastSeenTimestamp##description##15481"
                                        },
                                        "xdm:trusted": {
                                            "title": "Trusted Data Source Flag",
                                            "description": "Indicator of whether the data source is trusted or not.",
                                            "type": "boolean",
                                            "default": true,
                                            "meta:titleId": "graph-edge##xdm:trusted##title##64611",
                                            "meta:descriptionId": "graph-edge##xdm:trusted##description##11581"
                                        },
                                        "xdm:directed": {
                                            "title": "Directed Or Birectional",
                                            "description": "Nature of graph whether it is directed or birectional.",
                                            "type": "boolean",
                                            "default": false,
                                            "meta:titleId": "graph-edge##xdm:directed##title##79011",
                                            "meta:descriptionId": "graph-edge##xdm:directed##description##44031"
                                        },
                                        "xdm:relationType": {
                                            "title": "Relationship Type",
                                            "description": "The method that established the relationship - Online/Offline deterministic/probabilistic.",
                                            "type": "string",
                                            "enum": [
                                                "online_deterministic",
                                                "online_probabilistic",
                                                "offline_deterministic",
                                                "offline_probabilistic"
                                            ],
                                            "meta:enum": {
                                                "online_deterministic": "Online Deterministic",
                                                "online_probabilistic": "Online Probabilistic",
                                                "offline_deterministic": "Offline Deterministic",
                                                "offline_probabilistic": "Offline Probabilistic"
                                            },
                                            "meta:titleId": "graph-edge##xdm:relationType##title##93061",
                                            "meta:descriptionId": "graph-edge##xdm:relationType##description##97811"
                                        },
                                        "xdm:frequency": {
                                            "title": "Frequency",
                                            "description": "Frequency of the observed relationship.",
                                            "type": "integer",
                                            "meta:titleId": "graph-edge##xdm:frequency##title##45281",
                                            "meta:descriptionId": "graph-edge##xdm:frequency##description##10371"
                                        },
                                        "xdm:score": {
                                            "title": "Relationship Score",
                                            "description": "Imputed score of relationship.",
                                            "type": "number",
                                            "meta:titleId": "graph-edge##xdm:score##title##7031",
                                            "meta:descriptionId": "graph-edge##xdm:score##description##90791"
                                        },
                                        "xdm:summedRecency": {
                                            "title": "Recency Score",
                                            "description": "Recency score of relationship.",
                                            "type": "integer",
                                            "meta:titleId": "graph-edge##xdm:summedRecency##title##21221",
                                            "meta:descriptionId": "graph-edge##xdm:summedRecency##description##86001"
                                        },
                                        "xdm:lastCalculatedTimestamp": {
                                            "title": "Last Calculated Data And Time",
                                            "description": "Time and date the relationship score was last calculated.",
                                            "type": "string",
                                            "format": "date-time",
                                            "meta:titleId": "graph-edge##xdm:lastCalculatedTimestamp##title##37571",
                                            "meta:descriptionId": "graph-edge##xdm:lastCalculatedTimestamp##description##93331"
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "meta:titleId": "graph-edge##xdm:edges##title##7031",
                    "meta:descriptionId": "graph-edge##xdm:edges##description##96631"
                }
            }
        }
    },
    "allOf": [
        {
            "$ref": "https://ns.adobe.com/xdm/common/extensible#/definitions/@context"
        },
        {
            "$ref": "#/definitions/graph-edge"
        },
        {
            "$ref": "https://ns.adobe.com/xdm/common/auditable"
        }
    ],
    "meta:status": "stable",
    "meta:createdDate": "2020-08-10",
    "meta:titleId": "graph-edge##title##6631",
    "meta:descriptionId": "graph-edge##description##46101",
    "examples": [
        {
            "xdm:edges": [
                {
                    "xdm:sourceNodeId": "aam_1|0",
                    "xdm:targetNodeId": "dpuuid_1|10",
                    "xdm:dataProvider": [
                        {
                            "xdm:dataProviderId": 5,
                            "xdm:firstSeenTimestamp": "2019-02-01T15:52:25+00:00",
                            "xdm:lastSeenTimestamp": "2019-02-01T15:52:25+00:00",
                            "xdm:trusted": true,
                            "xdm:directed": false,
                            "xdm:relationType": "online_deterministic",
                            "xdm:frequency": 1,
                            "xdm:lastCalculatedTimestamp": "2019-02-01T15:52:25+00:00"
                        }
                    ]
                }
            ]
        }
    ]
}