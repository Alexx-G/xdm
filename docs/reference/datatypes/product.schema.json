    "title": "Product",
    "type": "object",
    "description": "XDM product name or title, master product, and key attributes of product in the product catalog.",
    "definitions": {
        "product": {
            "properties": {
                "@id": {
                    "title": "Identifier",
                    "type": "string",
                    "format": "uri-reference",
                    "description": "The internal unique ID of the product in the commerce backend system.",
                    "meta:titleId": "product##@id##title##96091",
                    "meta:descriptionId": "product##@id##description##91381"
                },
                "xdm:SKU": {
                    "title": "SKU",
                    "type": "string",
                    "description": "The unique SKU (stock keeping unit) of the product assigned by the vendor.",
                    "meta:descriptors": [
                        {
                            "@type": "xdm:searchdescriptor",
                            "search:indexed": true
                        }
                    ],
                    "meta:titleId": "product##xdm:SKU##title##73551",
                    "meta:descriptionId": "product##xdm:SKU##description##66041"
                },
                "xdm:name": {
                    "title": "Name",
                    "type": "string",
                    "description": "The name of the product.",
                    "meta:titleId": "product##xdm:name##title##23061",
                    "meta:descriptionId": "product##xdm:name##description##29911"
                },
                "schema:description": {
                    "title": "Description",
                    "type": "string",
                    "description": "The localized description of the product.",
                    "meta:status": "deprecated",
                    "meta:titleId": "product##schema:description##title##10701",
                    "meta:descriptionId": "product##schema:description##description##54331"
                },
                "xdm:productDescription": {
                    "title": "Description",
                    "type": "string",
                    "description": "The localized description of the product.",
                    "meta:descriptors": [
                        {
                            "@type": "xdm:searchdescriptor",
                            "search:indexed": true
                        }
                    ],
                    "meta:titleId": "product##xdm:productDescription##title##68001",
                    "meta:descriptionId": "product##xdm:productDescription##description##131"
                },
                "xdm:category": {
                    "title": "Category",
                    "type": "string",
                    "description": "Primary categorization or category name of the product.",
                    "meta:titleId": "product##xdm:category##title##66451",
                    "meta:descriptionId": "product##xdm:category##description##991"
                },
                "xdm:department": {
                    "title": "Department",
                    "type": "string",
                    "description": "Primary department the product is associated with.",
                    "meta:titleId": "product##xdm:department##title##24401",
                    "meta:descriptionId": "product##xdm:department##description##34771"
                },
                "xdm:brand": {
                    "title": "Brand",
                    "type": "string",
                    "description": "The brand of the product.",
                    "meta:descriptors": [
                        {
                            "@type": "xdm:searchdescriptor",
                            "search:indexed": true
                        }
                    ],
                    "meta:titleId": "product##xdm:brand##title##68801",
                    "meta:descriptionId": "product##xdm:brand##description##14351"
                },
                "xdm:masterProductID": {
                    "title": "Master product identifier",
                    "type": "string",
                    "format": "uri",
                    "description": "The internal unique ID of the product in the commerce backend system.",
                    "meta:titleId": "product##xdm:masterProductID##title##12231",
                    "meta:descriptionId": "product##xdm:masterProductID##description##58281"
                },
                "xdm:masterProductSKU": {
                    "title": "Master product SKU",
                    "type": "string",
                    "description": "The unique SKU (stock keeping unit) of the master product assigned by the vendor or manufacturer.",
                    "meta:titleId": "product##xdm:masterProductSKU##title##40841",
                    "meta:descriptionId": "product##xdm:masterProductSKU##description##83971"
                },
                "xdm:masterProductName": {
                    "title": "Master product name",
                    "type": "string",
                    "description": "The localized name of the product.",
                    "meta:titleId": "product##xdm:masterProductName##title##12941",
                    "meta:descriptionId": "product##xdm:masterProductName##description##38961"
                },
                "xdm:masterProductDescription": {
                    "title": "Master product description",
                    "type": "string",
                    "description": "The description of the product.",
