                    "meta:descriptionId": "product##xdm:fabrication##description##36521"
                },
                "xdm:material": {
                    "title": "Material",
                    "type": "string",
                    "description": "Primary material of the product construction.",
                    "meta:descriptors": [
                        {
                            "@type": "xdm:searchdescriptor",
                            "search:indexed": true
                        }
                    ],
                    "meta:titleId": "product##xdm:material##title##88871",
                    "meta:descriptionId": "product##xdm:material##description##55021"
                },
                "xdm:gender": {
                    "title": "Gender",
                    "type": "string",
                    "description": "Gender the product is marketed to.",
                    "meta:enum": {
                        "male": "For men",
                        "female": "For women",
                        "unisex": "For both men and women",
                        "unknown": "Unknown"
                    },
                    "meta:titleId": "product##xdm:gender##title##47381",
                    "meta:descriptionId": "product##xdm:gender##description##58681"
                },
                "xdm:size": {
                    "title": "Size",
                    "type": "number",
                    "description": "Standard product size for the product.",
                    "meta:titleId": "product##xdm:size##title##7241",
                    "meta:descriptionId": "product##xdm:size##description##50951"
                },
                "xdm:unitOfMeasure": {
                    "title": "Unit of measure",
                    "type": "string",
                    "description": "Standard unit of measure of the product and or variation and relates the units for the size measurement.",
                    "meta:titleId": "product##xdm:unitOfMeasure##title##39901",
                    "meta:descriptionId": "product##xdm:unitOfMeasure##description##14091"
                },
                "xdm:countryOfOrigin": {
                    "title": "Country of origin",
                    "type": "string",
                    "pattern": "^[A-Z]{2}$",
                    "description": "The two-character [ISO 3166-1 alpha-2](https://datahub.io/core/country-list) code for the country of origin of the product as defined by customs requirements.",
                    "meta:titleId": "product##xdm:countryOfOrigin##title##96401",
                    "meta:descriptionId": "product##xdm:countryOfOrigin##description##70581"
                },
                "xdm:COGS": {
                    "title": "Cost of good sold",
                    "type": "number",
                    "description": "Cost of good sold, in the `currencyCode` currency.",
                    "meta:titleId": "product##xdm:COGS##title##76031",
                    "meta:descriptionId": "product##xdm:COGS##description##99591"
                },
                "xdm:listPrice": {
                    "title": "List price",
                    "type": "number",
                    "description": "Default price of the product before sales and discounting, in the `currencyCode` currency.",
                    "meta:titleId": "product##xdm:listPrice##title##66501",
                    "meta:descriptionId": "product##xdm:listPrice##description##8401"
                },
                "xdm:currencyCode": {
                    "title": "Currency code",
                    "type": "string",
                    "examples": [
                        "USD",
                        "EUR"
                    ],
                    "pattern": "^[A-Z]{3}$",
                    "description": "The ISO 4217 alphabetic currency code used for cost and pricing, including `listPrice` and `COGS`.",
                    "meta:titleId": "product##xdm:currencyCode##title##33141",
                    "meta:descriptionId": "product##xdm:currencyCode##description##84111"
                },
                "xdm:originalSaleDate": {
                    "title": "Original sale date",
                    "type": "string",
                    "format": "date",
                    "description": "First date the product was made available for sale. The time using RFC3339 with a stated time zone offset such as \"2001-07-04T12:08:56-07:00\". An example formatting pattern is \"yyyy-MM-dd'T'HH:mm:ssXXX\".",
                    "meta:titleId": "product##xdm:originalSaleDate##title##40681",
                    "meta:descriptionId": "product##xdm:originalSaleDate##description##73471"
                },
                "xdm:productCreateDate": {
                    "title": "Product creation date",
                    "format": "date",
                    "type": "string",
                    "description": "The date when this product was created. The time using RFC3339 with a stated time zone offset such as \"2001-07-04T12:08:56-07:00\". An example formatting pattern is \"yyyy-MM-dd'T'HH:mm:ssXXX\".",
                    "meta:titleId": "product##xdm:productCreateDate##title##50171",
                    "meta:descriptionId": "product##xdm:productCreateDate##description##57051"
                },
                "xdm:productLastModified": {
                    "title": "Product last modified",
                    "format": "date",
                    "type": "string",
                    "description": "The date when this product was last modified. The time using RFC3339 with a stated time zone offset such as \"2001-07-04T12:08:56-07:00\". An example formatting pattern is \"yyyy-MM-dd'T'HH:mm:ssXXX\".",
                    "meta:titleId": "product##xdm:productLastModified##title##57381",
                    "meta:descriptionId": "product##xdm:productLastModified##description##47281"
                },
                "xdm:productURL": {
                    "title": "Product URL",
                    "type": "string",
                    "description": "The URL for the primary product view of the product page.",
                    "meta:titleId": "product##xdm:productURL##title##59251",
                    "meta:descriptionId": "product##xdm:productURL##description##56301"
                },
                "xdm:manufacturerName": {
                    "title": "Manufacturer name",
                    "type": "string",
                    "description": "Manufacturer of the product.",
                    "meta:titleId": "product##xdm:manufacturerName##title##17191",
                    "meta:descriptionId": "product##xdm:manufacturerName##description##34571"
                },
                "xdm:supplierName": {
                    "title": "Supplier name",
                    "type": "string",
                    "description": "The distributor of the product.",
                    "meta:titleId": "product##xdm:supplierName##title##77711",
                    "meta:descriptionId": "product##xdm:supplierName##description##87161"
                }
            }
        }
    },
