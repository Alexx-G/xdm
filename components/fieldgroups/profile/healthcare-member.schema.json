  "$id": "https://ns.adobe.com/xdm/mixins/healthcare-member",
  "$schema": "http://json-schema.org/draft-06/schema#",
  "title": "Member Details",
  "type": "object",
  "meta:extensible": true,
  "meta:abstract": true,
  "meta:tags": {
    "industry": ["healthcare_services"]
  },
  "meta:intendedToExtend": ["https://ns.adobe.com/xdm/context/profile"],
  "description": "Details of a person who may be waiting for a care or may be receiving care or may have already received care.",
  "definitions": {
    "member-details": {
      "properties": {
        "xdm:member": {
          "type": "object",
          "properties": {
            "xdm:ID": {
              "Title": "Member ID",
              "type": "string",
              "description": "Unique Identifier of the patient/member.",
              "meta:descriptionId": "healthcare-member##xdm:ID##description##47591"
            },
            "xdm:name": {
              "Title": "Member Name",
              "description": "Member's full name.",
              "$ref": "https://ns.adobe.com/xdm/context/person-name",
              "meta:descriptionId": "healthcare-member##xdm:name##description##29301"
            },
            "xdm:memberAddress": {
              "type": "object",
              "properties": {
                "xdm:addressType": {
                  "Title": "Member Address type",
                  "type": "string",
                  "enum": ["Home", "Work", "Mailing", "Other"],
                  "meta:enum": {
                    "Home": "Home Address",
                    "Work": "Work Address",
                    "Mailing": "Mailing Address",
                    "Other": "Other Address"
                  }
                },
                "xdm:address": {
                  "Title": "Member Address",
                  "$ref": "https://ns.adobe.com/xdm/common/address"
                },
                "xdm:effectiveStartDate": {
                  "Title": "Effective Start Date",
                  "type": "string",
                  "format": "date-time",
                  "description": "Address start date.",
                  "meta:descriptionId": "healthcare-member##xdm:effectiveStartDate##description##30541"
                },
                "xdm:efectiveEndDate": {
                  "Title": "Effective End Date",
                  "type": "string",
                  "format": "date-time",
                  "description": "Address end date.",
                  "meta:descriptionId": "healthcare-member##xdm:efectiveEndDate##description##53781"
                }
              },
              "xdm:phoneNumber": {
                "Title": "Phone Number",
                "description": "Member's phone number.",
                "$ref": "https://ns.adobe.com/xdm/context/phonenumber",
                "meta:descriptionId": "healthcare-member##xdm:phoneNumber##description##901"
              },
              "xdm:memberDetails": {
                "Title": "Member Details",
                "description": "Member's additional details.",
                "type": "object",
                "properties": {
                  "xdm:birthDate": {
                    "title": "Birth date(YYYY-MM-DD)",
                    "type": "string",
                    "format": "date",
                    "description": "The full date a person was born.",
                    "meta:titleId": "healthcare-member##xdm:birthDate##title##5671",
                    "meta:descriptionId": "healthcare-member##xdm:birthDate##description##68161"
                  },
                  "xdm:gender": {
                    "title": "Gender",
                    "type": "string",
                    "enum": ["male", "female", "not_specified", "non_specific"],
                    "meta:enum": {
                      "male": "Male",
                      "female": "Female",
                      "not_specified": "Not Specified",
                      "non_specific": "Non-specific"
                    },
                    "description": "Gender identity of the person.\n",
                    "default": "not_specified",
                    "meta:titleId": "healthcare-member##xdm:gender##title##12751",
                    "meta:descriptionId": "healthcare-member##xdm:gender##description##85901"
                  },
                  "xdm:maritalStatus": {
                    "title": "Marital Status",
                    "type": "string",
                    "enum": [
                      "married",
                      "single",
                      "divorced",
                      "widowed",
                      "not_specified"
                    ],
                    "meta:enum": {
                      "married": "Married",
                      "single": "Single",
                      "divorced": "Divorced",
                      "widowed": "Widowed",
                      "not_specified": "Not Specified"
                    },
                    "description": "Describes a person's relationship with a significant other.",
                    "default": "not_specified",
                    "meta:titleId": "healthcare-member##xdm:maritalStatus##title##67211",
                    "meta:descriptionId": "healthcare-member##xdm:maritalStatus##description##43711"
                  },
                  "xdm:nationality": {
                    "title": "Nationality",
                    "type": "string",
                    "pattern": "^[A-Z]{2}$",
                    "description": "The legal relationship between a person and their state represented using the ISO 3166-1 Alpha-2 code.",
                    "meta:titleId": "healthcare-member##xdm:nationality##title##16271",
                    "meta:descriptionId": "healthcare-member##xdm:nationality##description##43241"
                  },
                  "xdm:dateAgeCollected": {
                    "Title": "Date Age Collected",
                    "type": "string",
                    "format": "date-time",
                    "description": "Date when date of birth information collected.",
                    "meta:descriptionId": "healthcare-member##xdm:dateAgeCollected##description##62451"
                  },
                  "xdm:multipleBirth": {
                    "type": "object",
                    "properties": {
                      "xdm:isMultipleBirth ": {
                        "Title": "Is Multiple Birth",
                        "type": "boolean",
                        "description": "Boolean flag to indicate if the member had a multiple birth.",
                        "meta:descriptionId": "healthcare-member##xdm:isMultipleBirth ##description##53951"
                      },
                      "xdm:multipleBirthNumber": {
                        "Title": "Multiple Birth Number",
                        "type": "integer",
                        "description": "Number of babies born if multiple birth is true.",
                        "meta:descriptionId": "healthcare-member##xdm:multipleBirthNumber##description##89501"
                      }
                    }
                  }
                },
                "meta:descriptionId": "healthcare-member##xdm:memberDetails##description##33161"
              },
              "xdm:emailAddress": {
                "Title": "Email Address",
                "$ref": "https://ns.adobe.com/xdm/context/emailaddress"
              },
              "xdm:memberOrganization": {
                "Title": "Member Organization",
                "$ref": "https://ns.adobe.com/xdm/datatypes/b2b-account-organization"
              }
            }
          }
        },
        "xdm:primaryCarePhysician ": {
          "type": "array",
          "items": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
              "xdm:ID ": {
                "Title": "ID",
                "type": "string",
                "description": "Primary Care Physician unique identifier.",
                "meta:descriptionId": "healthcare-member##xdm:ID ##description##15991"
              },
              "xdm:name": {
                "Title": "Primary Care Physician Name",
                "type": "string",
                "description": "Primary Care Physician Name.",
                "meta:descriptionId": "healthcare-member##xdm:name##description##87221"
              },
              "xdm:startDate": {
                "Title": "Start Date",
                "type": "string",
                "format": "date-time",
                "description": "Primary Care Physician start date.",
                "meta:descriptionId": "healthcare-member##xdm:startDate##description##16251"
              },
              "xdm:endDate": {
                "Title": "End Date",
                "type": "string",
                "format": "date-time",
                "description": "Primary Care Physician end date.",
                "meta:descriptionId": "healthcare-member##xdm:endDate##description##94361"
              }
            }
          }
        },
        "xdm:emergencyContact": {
          "type": "object",
          "properties": {
            "xdm:name": {
              "Title": "Emergency Contact Name",
              "type": "string",
              "description": "Emergency contact name.",
              "meta:descriptionId": "healthcare-member##xdm:name##description##42541"
            },
            "xdm:contactPhone": {
              "Title": "Emergency Contact Phone",
              "type": "string",
              "description": "Emergency contact phone number.",
              "meta:descriptionId": "healthcare-member##xdm:contactPhone##description##6981"
            }
          }
        },
        "xdm:prefferedAvaiability": {
          "Title": "Preffered Availability",
          "type": "string",
          "description": "Patient prefered day and time avaiability for an appointment.",
          "meta:descriptionId": "healthcare-member##xdm:prefferedAvaiability##description##76751"
        },
        "xdm:Medication": {
          "type": "array",
          "items": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
              "xdm:medicationID": {
                "Title": "Medication Id",
                "type": "string",
                "description": "Medication unique identifier.",
                "meta:descriptionId": "healthcare-member##xdm:medicationID##description##69831"
              },
              "xdm:medicationDate": {
                "Title": "Medication Date",
                "type": "string",
                "format": "date-time",
                "description": "Medication Date.",
                "meta:descriptionId": "healthcare-member##xdm:medicationDate##description##97351"
              },
              "xdm:isCurrent": {
                "Title": "Is Current",
                "type": "boolean",
                "description": "Boolean flag indicates if this medication is current or previous.",
                "meta:descriptionId": "healthcare-member##xdm:isCurrent##description##64491"
              }
            }
          }
        },
        "xdm:dependent": {
          "Title": "Dependent",
          "type": "integer",
          "description": "number issued to each beneficiary.",
          "meta:descriptionId": "healthcare-member##xdm:dependent##description##46921"
        },
        "xdm:beneficiaryRelationship": {
          "Title": "Beneficiary Relationship",
          "type": "string",
          "description": "Beneficiary relationship to the Subscriber if the member is a dependent(self,spouse,child..etc).",
          "meta:descriptionId": "healthcare-member##xdm:beneficiaryRelationship##description##64991"
        },
        "xdm:primaryMemeberID": {
          "Title": "Primary Member ID",
          "type": "string",
          "description": "Unique Identifier of the primary subscriber if the member is dependent.",
          "meta:descriptionId": "healthcare-member##xdm:primaryMemeberID##description##49591"
        },
        "xdm:billingAccountID": {
          "Title": "Billing Account ID",
          "type": "string",
          "description": "Unique Identifier of the billing account.",
          "meta:descriptionId": "healthcare-member##xdm:billingAccountID##description##99071"
        },
        "xdm:specialist": {
          "Title": "Specialist",
          "type": "array",
          "items": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
              "xdm:name": {
                "Title": "Name",
                "type": "string",
                "description": "Name of the specialist.",
                "meta:descriptionId": "healthcare-member##xdm:name##description##50151"
              },
              "xdm:diagnosis": {
                "Title": "Diagnosis",
                "type": "string",
                "description": "Specialization diagnosis.",
                "meta:descriptionId": "healthcare-member##xdm:diagnosis##description##16121"
              },
              "xdm:providerID": {
                "Title": "Providerid",
                "type": "string",
                "description": "Provider unique identifier.",
                "meta:descriptionId": "healthcare-member##xdm:providerID##description##4021"
              }
            }
          }
        },
        "xdm:isLead": {
          "Title": "Is Lead",
          "type": "boolean",
          "description": "Boolean flag indicates if this member is a lead.",
          "meta:descriptionId": "healthcare-member##xdm:isLead##description##46391"
        },
        "xdm:isOpportunity": {
          "Title": "Is Opportunity",
          "type": "boolean",
          "description": "Boolean flag indicates if this this member is a opportunity.",
          "meta:descriptionId": "healthcare-member##xdm:isOpportunity##description##34551"
        },
        "xdm:plan": {
          "Title": "Member Plan",
          "type": "array",
          "items": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
              "xdm:ID": {
                "Title": "ID",
                "type": "string",
                "description": "Plan ID unique identifier.",
                "meta:descriptionId": "healthcare-member##xdm:ID##description##98951"
              },
              "xdm:coverageType": {
                "Title": "Coverage Type",
                "type": "string",
                "enum": ["Medical", "Dental", "Vision", "Accident"],
                "meta:enum": {
                  "Medical": "Medical",
                  "Dental": "Dental",
                  "Vision": "Vision",
                  "Accident": "Accident"
                },
                "description": "Member plan coverage type.",
                "meta:descriptionId": "healthcare-member##xdm:coverageType##description##98671"
              },
              "xdm:payerID": {
                "Title": "Payer ID",
                "type": "string",
                "description": "Payer unique identifier.",
                "meta:descriptionId": "healthcare-member##xdm:payerID##description##85381"
              },
              "xdm:contractID": {
                "Title": "Contract ID",
                "type": "string",
                "description": "Unique identifier of the contract that is signed by the member.",
                "meta:descriptionId": "healthcare-member##xdm:contractID##description##62461"
              },
              "xdm:ownerID": {
                "Title": "Owner ID",
                "type": "string",
                "description": "Unique identifier of the Owner of the policy which could be owned by an individual , group or an organization.",
                "meta:descriptionId": "healthcare-member##xdm:ownerID##description##44951"
              },
              "xdm:isActive": {
                "Title": "Is Active",
                "type": "boolean",
                "description": "Boolean flag indicates whether the plan is active.",
                "meta:descriptionId": "healthcare-member##xdm:isActive##description##87471"
              },
              "xdm:network": {
                "Title": "Network",
                "type": "string",
                "description": "The insurer-specific identifier for the insurer-defined network of providers to which the beneficiary may seek treatment which will be covered at the 'in-network' rate, otherwise 'out of network' term.",
                "meta:descriptionId": "healthcare-member##xdm:network##description##3831"
              },
              "xdm:affiliation": {
                "Title": "Affiliation",
                "type": "string",
                "description": "An affiliation to a government service, such as the army or navy..",
                "meta:descriptionId": "healthcare-member##xdm:affiliation##description##93761"
              },
              "xdm:coverageStartDate": {
                "Title": "Coverage Start Date",
                "type": "string",
                "format": "date-time",
                "description": "Coverage start date.",
                "meta:descriptionId": "healthcare-member##xdm:coverageStartDate##description##14801"
              },
              "xdm:coverageEndDate": {
                "Title": "Coverage End Date",
                "type": "string",
                "format": "date-time",
                "description": "Coverage end date.",
                "meta:descriptionId": "healthcare-member##xdm:coverageEndDate##description##13171"
              },
              "xdm:lastVerification": {
                "Title": "Last Verification Date",
                "type": "string",
                "format": "date-time",
                "description": "The date on which this plan was last verified.",
                "meta:descriptionId": "healthcare-member##xdm:lastVerification##description##27461"
              },
              "xdm:planType": {
                "Title": "Plan Type",
                "type": "string",
                "enum": ["primary", "secondary", "tertiary"],
                "meta:enum": {
                  "primary": "primary",
                  "secondary": "secondary",
                  "tertiary": "tertiary"
                },
                "description": "This indicates if the plan is primary,secondary or tertiary plan.",
                "meta:descriptionId": "healthcare-member##xdm:planType##description##78481"
              }
            }
          }
        }
      }
    }
  },
  "allOf": [
