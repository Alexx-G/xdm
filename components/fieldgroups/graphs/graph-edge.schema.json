  "$id": "https://ns.adobe.com/xdm/context/graph-edge",
  "$schema": "http://json-schema.org/draft-06/schema#",
  "title": "XDM Graph Edge",
  "type": "object",
  "description": "Information regarding the relationship between two nodes or vertices such as data provider, and edge information.",
  "meta:extensible": true,
  "meta:abstract": true,
  "meta:intendedToExtend": ["https://ns.adobe.com/xdm/context/graphs"],
  "definitions": {
    "graph-edge": {
      "properties": {
        "xdm:edges": {
          "title": "Array Of Edges",
          "description": "Array of edges.",
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "xdm:edgeId": {
                "title": "Edge Identifier",
                "description": "The unique identifier of the identity associated with this graph edge.",
                "type": "string",
                "meta:titleId": "graph-edge##xdm:edgeId##title##3101",
                "meta:descriptionId": "graph-edge##xdm:edgeId##description##92971"
              },
              "xdm:sourceNodeId": {
                "title": "Source Node Id",
                "description": "Source node identifier.",
                "type": "string",
                "meta:titleId": "graph-edge##xdm:sourceNodeId##title##47781",
                "meta:descriptionId": "graph-edge##xdm:sourceNodeId##description##89561"
              },
              "xdm:targetNodeId": {
                "title": "Target Node Id",
                "description": "Target node identifier.",
                "type": "string",
                "meta:titleId": "graph-edge##xdm:targetNodeId##title##8711",
                "meta:descriptionId": "graph-edge##xdm:targetNodeId##description##10261"
              },
              "xdm:relation": {
                "title": "Relation Description",
                "description": "Relation description.",
                "type": "string",
                "meta:titleId": "graph-edge##xdm:relation##title##77161",
                "meta:descriptionId": "graph-edge##xdm:relation##description##48401"
              },
              "xdm:label": {
                "title": "Label",
                "description": "Additional description.",
                "type": "string",
                "meta:titleId": "graph-edge##xdm:label##title##6441",
                "meta:descriptionId": "graph-edge##xdm:label##description##94671"
              },
              "xdm:dataProvider": {
                "type": "array",
                "items": {
                  "type": "object",
                  "additionalProperties": false,
                  "properties": {
                    "xdm:dataProviderId": {
                      "title": "Data Provider Id",
                      "description": "Identifier of the Data Provider..",
                      "type": "integer",
                      "meta:titleId": "graph-edge##xdm:dataProviderId##title##26561",
                      "meta:descriptionId": "graph-edge##xdm:dataProviderId##description##6871"
                    },
                    "xdm:firstSeenTimestamp": {
                      "title": "Relationship Observed Time and Date",
                      "description": "Time and date the relation was first observed.",
                      "type": "string",
                      "format": "date-time",
                      "meta:titleId": "graph-edge##xdm:firstSeenTimestamp##title##90741",
                      "meta:descriptionId": "graph-edge##xdm:firstSeenTimestamp##description##93011"
                    },
                    "xdm:lastSeenTimestamp": {
                      "title": "Relationship Observed most recent Time and Date",
                      "description": "Time and date the relation was last observed.",
                      "type": "string",
                      "format": "date-time",
                      "meta:titleId": "graph-edge##xdm:lastSeenTimestamp##title##85531",
                      "meta:descriptionId": "graph-edge##xdm:lastSeenTimestamp##description##68961"
                    },
                    "xdm:trusted": {
                      "title": "Trusted Data Source Flag",
                      "description": "Indicator of whether the data source is trusted or not.",
                      "type": "boolean",
                      "default": true,
                      "meta:titleId": "graph-edge##xdm:trusted##title##63051",
                      "meta:descriptionId": "graph-edge##xdm:trusted##description##11351"
                    },
                    "xdm:directed": {
                      "title": "Directed Or Birectional",
                      "description": "Nature of graph whether it is directed or birectional.",
                      "type": "boolean",
                      "default": false,
                      "meta:titleId": "graph-edge##xdm:directed##title##93141",
                      "meta:descriptionId": "graph-edge##xdm:directed##description##20601"
                    },
                    "xdm:relationType": {
                      "title": "Relationship Type",
                      "description": "The method that established the relationship - Online/Offline deterministic/probabilistic.",
                      "type": "string",
                      "enum": [
                        "online_deterministic",
                        "online_probabilistic",
                        "offline_deterministic",
                        "offline_probabilistic"
                      ],
                      "meta:enum": {
                        "online_deterministic": "Online Deterministic",
                        "online_probabilistic": "Online Probabilistic",
                        "offline_deterministic": "Offline Deterministic",
                        "offline_probabilistic": "Offline Probabilistic"
                      },
                      "meta:titleId": "graph-edge##xdm:relationType##title##81381",
                      "meta:descriptionId": "graph-edge##xdm:relationType##description##85111"
                    },
                    "xdm:frequency": {
                      "title": "Frequency",
                      "description": "Frequency of the observed relationship.",
                      "type": "integer",
                      "meta:titleId": "graph-edge##xdm:frequency##title##23181",
                      "meta:descriptionId": "graph-edge##xdm:frequency##description##89091"
                    },
                    "xdm:score": {
                      "title": "Relationship Score",
                      "description": "Imputed score of relationship.",
                      "type": "number",
                      "meta:titleId": "graph-edge##xdm:score##title##33091",
                      "meta:descriptionId": "graph-edge##xdm:score##description##59481"
                    },
                    "xdm:summedRecency": {
                      "title": "Recency Score",
                      "description": "Recency score of relationship.",
                      "type": "integer",
                      "meta:titleId": "graph-edge##xdm:summedRecency##title##63841",
                      "meta:descriptionId": "graph-edge##xdm:summedRecency##description##1691"
                    },
                    "xdm:lastCalculatedTimestamp": {
                      "title": "Last Calculated Data And Time",
                      "description": "Time and date the relationship score was last calculated.",
                      "type": "string",
                      "format": "date-time",
                      "meta:titleId": "graph-edge##xdm:lastCalculatedTimestamp##title##84531",
                      "meta:descriptionId": "graph-edge##xdm:lastCalculatedTimestamp##description##75371"
                    }
                  }
                }
              }
            }
          },
          "meta:titleId": "graph-edge##xdm:edges##title##39171",
          "meta:descriptionId": "graph-edge##xdm:edges##description##20361"
        }
      }
    }
  },
  "allOf": [
    {
      "$ref": "https://ns.adobe.com/xdm/common/extensible#/definitions/@context"
    },
    {
      "$ref": "#/definitions/graph-edge"
    },
    {
      "$ref": "https://ns.adobe.com/xdm/common/auditable"
    }
  ],
  "meta:status": "stable",
  "meta:createdDate": "2020-08-10",
  "meta:titleId": "graph-edge##title##79121",
  "meta:descriptionId": "graph-edge##description##77901"
}
