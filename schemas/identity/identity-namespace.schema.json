  "$id": "https://ns.adobe.com/xdm/context/identity-namespace",
  "$schema": "http://json-schema.org/draft-06/schema#",
  "title": "Identity Namespace",
  "type": "object",
  "meta:extensible": false,
  "meta:abstract": false,
  "meta:extends": ["https://ns.adobe.com/xdm/data/record"],
  "description": "Identity Namespace Schema.",
  "definitions": {
    "identity-namespace": {
      "properties": {
        "xdm:namespaceID": {
          "title": "Unique Namespace ID",
          "type": "integer",
          "description": "Unique Identifier for namespace",
          "meta:titleId": "identity-namespace##xdm:namespaceID##title##9501",
          "meta:descriptionId": "identity-namespace##xdm:namespaceID##description##51331"
        },
        "xdm:namespaceName": {
          "title": "Namespace Name",
          "type": "string",
          "description": "This is human readable name for the namespace",
          "meta:titleId": "identity-namespace##xdm:namespaceName##title##3611",
          "meta:descriptionId": "identity-namespace##xdm:namespaceName##description##73291"
        },
        "xdm:namespaceCode": {
          "title": "Namespace Code",
          "type": "string",
          "description": "This code is a human readable identifier for the namespace",
          "meta:titleId": "identity-namespace##xdm:namespaceCode##title##8311",
          "meta:descriptionId": "identity-namespace##xdm:namespaceCode##description##71411"
        },
        "xdm:namespaceDescription": {
          "title": "Namespace Description",
          "type": "string",
          "description": "Description for the namespace",
          "meta:titleId": "identity-namespace##xdm:namespaceDescription##title##40291",
          "meta:descriptionId": "identity-namespace##xdm:namespaceDescription##description##64871"
        },
        "xdm:namespaceIdType": {
          "title": "Namespace Id Type",
          "type": "string",
          "description": "Identity identifier type for the namespace",
          "enum": [
            "EMAIL",
            "PHONE_NUMBER",
            "DEVICE",
            "COOKIE",
            "CROSS_DEVICE",
            "NON_PEOPLE"
          ],
          "meta:enum": {
            "EMAIL": "Email",
            "PHONE_NUMBER": "Phone number",
            "DEVICE": "Device ID",
            "COOKIE": "Cookie ID",
            "CROSS_DEVICE": "Cross-device ID",
            "NON_PEOPLE": "Non-people identifier"
          },
          "meta:titleId": "identity-namespace##xdm:namespaceIdType##title##29791",
          "meta:descriptionId": "identity-namespace##xdm:namespaceIdType##description##80421"
        },
        "xdm:createDate": {
          "title": "Create Date Time",
          "format": "date-time",
          "type": "string",
          "examples": ["An example form is \"2004-10-23T12:00:00-06:00\"."],
          "description": "The date and time when the namespace was created in ISO 8601 format",
          "meta:titleId": "identity-namespace##xdm:createDate##title##71921",
          "meta:descriptionId": "identity-namespace##xdm:createDate##description##75301"
        },
        "xdm:modifyDate": {
          "title": "Update Date Time",
          "format": "date-time",
          "type": "string",
          "examples": ["An example form is \"2004-10-23T12:00:00-06:00\"."],
          "description": "The date and time when the namespace was updated in ISO 8601 format",
          "meta:titleId": "identity-namespace##xdm:modifyDate##title##29711",
          "meta:descriptionId": "identity-namespace##xdm:modifyDate##description##81741"
        }
      },
      "required": ["xdm:namespaceID", "xdm:namespaceCode"]
    }
  },
  "allOf": [
    {
      "$ref": "https://ns.adobe.com/xdm/data/record"
    },
    {
      "$ref": "#/definitions/identity-namespace"
